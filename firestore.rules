rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // Development rule: Allow read/write access to all documents
    // for any authenticated user.
    // This is insecure and should be replaced with more granular rules
    // before production.
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
